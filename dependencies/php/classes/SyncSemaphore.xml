<?xml version="1.0" encoding="UTF-8" ?>
<class xmlns="http://xml.phpdox.net/src" full="SyncSemaphore" namespace="" name="SyncSemaphore">
  <constructor name="__construct" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Constructs a new SyncSemaphore object"/>
      <return type="void"/>
    </docblock>
    <parameter name="name" optional="true" byreference="false" type="string"/>
    <parameter name="initialval" optional="true" byreference="false" type="object" class="integer"/>
    <parameter name="autounlock" optional="true" byreference="false" type="object" class="bool"/>
  </constructor>
  <method name="lock" abstract="false" static="false" visibility="public" final="false">
