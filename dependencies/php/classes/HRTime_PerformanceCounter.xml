<?xml version="1.0" encoding="UTF-8" ?>
<class xmlns="http://xml.phpdox.net/src" full="HRTime\PerformanceCounter" namespace="" name="HRTime\PerformanceCounter">
  <method name="getFrequency" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Timer frequency in ticks per second"/>
      <return type="int"/>
    </docblock>
  </method>
  <method name="getTicks" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Current ticks from the system"/>
      <return type="int"/>
    </docblock>
  </method>
  <method name="getTicksSince" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Ticks elapsed since the given value"/>
      <return type="int"/>
    </docblock>
    <parameter name="start" optional="false" byreference="false" type="object" class="integer"/>
  </method>
</class>