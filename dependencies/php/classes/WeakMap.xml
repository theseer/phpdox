<?xml version="1.0" encoding="UTF-8" ?>
<class xmlns="http://xml.phpdox.net/src" full="WeakMap" namespace="" name="WeakMap">
  <implements name="Countable" full="Countable"/>
  <constructor name="__construct" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Constructs a new map"/>
      <return type="void"/>
    </docblock>
  </constructor>
  <method name="count" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Counts the number of live entries in the map"/>
      <return type="int"/>
    </docblock>
  </method>
  <method name="current" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Returns the current value under iteration"/>
      <return type="mixed"/>
    </docblock>
  </method>
  <method name="key" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Returns the current key under iteration."/>
      <return type="object"/>
    </docblock>
  </method>
  <method name="next" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Advances to the next map element"/>
      <return type="void"/>
    </docblock>
  </method>
  <method name="offsetExists" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Checks whether a certain object is in the map"/>
      <return type="bool"/>
    </docblock>
    <parameter name="object" optional="false" byreference="false" type="object" class="object"/>
  </method>
  <method name="offsetGet" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Returns the value pointed to by a certain object"/>
      <return type="mixed"/>
    </docblock>
    <parameter name="object" optional="false" byreference="false" type="object" class="object"/>
  </method>
  <method name="offsetSet" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Updates the map with a new key-value pair"/>
      <return type="void"/>
    </docblock>
    <parameter name="object" optional="false" byreference="false" type="object" class="object"/>
    <parameter name="value" optional="false" byreference="false" type="object" class="mixed"/>
  </method>
  <method name="offsetUnset" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Removes an entry from the map"/>
      <return type="void"/>
    </docblock>
    <parameter name="object" optional="false" byreference="false" type="object" class="object"/>
  </method>
  <method name="rewind" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Rewinds the iterator to the beginning of the map"/>
      <return type="void"/>
    </docblock>
  </method>
  <method name="valid" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Returns whether the iterator is still on a valid map element"/>
      <return type="bool"/>
    </docblock>
  </method>
</class>